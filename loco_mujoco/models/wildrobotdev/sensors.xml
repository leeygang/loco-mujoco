<sensor>
    <!-- Hardware capability-->
    <gyro site="chest_mimic" name="chest_gyro"/>
    <accelerometer site="chest_mimic" name="chest_accelerometer"/>
    <magnetometer site="chest_mimic" name="chest_magnetometer"/>

    <!-- Read from simulator -->
    <velocimeter site="chest_mimic" name="chest_local_linvel"/>
    <framequat objtype="site" objname="chest_mimic" name="chest_orientation"/>
    <framezaxis objtype="site" objname="chest_mimic" name="chest_upvector"/>
    <framexaxis objtype="site" objname="chest_mimic" name="chest_forwardvector"/>
    <framelinvel objtype="site" objname="chest_mimic" name="chest_global_linvel"/>
    <frameangvel objtype="site" objname="chest_mimic" name="chest_global_angvel"/>
</sensor>